{
    "name": "rubloge/laravel-wc-locations",
    "description": "List Countries and States using WooCommerce data",
    "keywords": [
        "laravel",
        "woocommerce",
        "countries",
        "states",
        "locations",
        "laravel-wc-locations"
    ],
    "homepage": "https://github.com/rubloge/laravel-wc-locations",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rubén López Gea",
            "email": "contactar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Rubloge\\LaravelWcLocations\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rubloge\\LaravelWcLocations\\LaravelWcLocationsServiceProvider"
            ],
            "aliases": {
                "LaravelWcLocations": "Rubloge\\LaravelWcLocations\\LaravelWcLocationsFacade"
            }
        }
    },
    "require-dev": {
        "laravel/pint": "^1.13"
    }
}